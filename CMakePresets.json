{
  "version": 8,
  "$schema": "https://cmake.org/cmake/help/latest/_downloads/3e2d73bff478d88a7de0de736ba5e361/schema.json",
  "configurePresets": [
    {
      "name": "zig",
      "displayName": "Zig",
      "generator": "Ninja",
      "binaryDir": "build",
      "cacheVariables": {
          "CMAKE_C_COMPILER": "zig",
          "CMAKE_CXX_COMPILER": "zig"
      }
    },
    {
      "name": "gcc",
      "displayName": "GCC",
      "generator": "Ninja",
      "binaryDir": "build",
      "cacheVariables": {
          "CMAKE_C_COMPILER": "gcc",
          "CMAKE_CXX_COMPILER": "g++"
      }
    },
    {
      "name": "clang",
      "displayName": "Clang",
      "generator": "Ninja",
      "binaryDir": "build",
      "cacheVariables": {
          "CMAKE_C_COMPILER": "clang",
          "CMAKE_CXX_COMPILER": "clang++"
      }
    }
  ]
}
